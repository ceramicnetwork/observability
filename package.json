  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.103",
    "@swc/jest": "0.2.29",
    "jest": "29.7.0",
    "prettier": "3.0.0",
    "turbo": "1.10.12",
    "typescript": "5.3.3"
  }
}
